{
  "name": "@dynamic-quants/config-tools",
  "version": "3.0.10",
  "description": "Configurations for tools used by the Dynamic Quants Team",
  "author": "Dynamic Quants",
  "license": "MIT",
  "keywords": [
    "config",
    "tools",
    "eslint",
    "jest",
    "playwright",
    "prettier",
    "vite",
    "tailwind",
    "nextjs",
    "react",
    "typescript",
    "nestjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DynamicQuants/config-tools.git"
  },
  "bin": {
    "@dynamic-quants/config-tools": "./bin/main.mjs"
  },
  "exports": {
    "./eslint": {
      "import": "./lib/eslint.mjs",
      "require": "./lib/eslint.js"
    },
    "./jest": {
      "import": "./lib/jest.mjs",
      "require": "./lib/jest.js"
    },
    "./playwright": {
      "import": "./lib/playwright.mjs",
      "require": "./lib/playwright.js"
    },
    "./prettier": {
      "import": "./lib/prettier.mjs",
      "require": "./lib/prettier.js"
    },
    "./vite": {
      "import": "./lib/vite.mjs",
      "require": "./lib/vite.js"
    },
    "./tailwind": {
      "import": "./lib/tailwind.mjs",
      "require": "./lib/tailwind.js"
    },
    "./tsconfig/*.json": "./tsconfig/*.json"
  },
  "typesVersions": {
    "*": {
      "eslint": [
        "./lib/eslint.d.ts"
      ],
      "jest": [
        "./lib/jest.d.ts"
      ],
      "playwright": [
        "./lib/playwright.d.ts"
      ],
      "prettier": [
        "./lib/prettier.d.ts"
      ],
      "tailwind": [
        "./lib/tailwind.d.ts"
      ],
      "vite": [
        "./lib/vite.d.ts"
      ]
    }
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,ts,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,md,json}\"",
    "cp": "copyfiles -f ./src/lib/tsconfig/*.json ./dist/tsconfig && copyfiles -f ./src/lib/jest/snapshotResolver.js ./dist/lib && copyfiles -f ./package.json ./README.md ./LICENSE ./dist && copyfiles -f ./src/bin/peers.json ./dist/bin",
    "build": "tsup"
  },
  "dependencies": {
    "find-root": "^1.1.0",
    "inquirer": "^12.2.0",
    "semver": "^7.6.3",
    "tsup": "^8.3.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@nx/eslint": "^20.2.2",
    "@nx/eslint-plugin": "^20.2.2",
    "@nx/jest": "^20.2.2",
    "@nx/next": "^20.2.2",
    "@nx/react": "^20.2.2",
    "@nx/vite": "^20.2.2",
    "@playwright/test": "^1.49.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/find-root": "^1.1.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonc-eslint-parser": "^2.4.0",
    "next": "15.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-multiline-arrays": "^3.0.6",
    "tailwindcss": "3.4.15",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "~4.3.0",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@eslint/compat": "^1.2.3",
    "@nx/eslint": "^20.2.2",
    "@nx/eslint-plugin": "^20.2.2",
    "@nx/jest": "^20.2.2",
    "@nx/next": "^20.2.2",
    "@nx/react": "^20.2.2",
    "@nx/vite": "^20.2.2",
    "@playwright/test": "^1.49.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonc-eslint-parser": "^2.4.0",
    "next": "15.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-multiline-arrays": "^3.0.6",
    "tailwindcss": "3.4.15",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "~4.3.0",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "./src/lib/prettier.mjs"
}
